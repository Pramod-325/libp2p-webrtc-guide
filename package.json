{
  "name": "libp2p-webrtc-guide",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "associated code for the libp2p webrtc guide",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:dev": "mkdir -p dist && ln -fs ../src/index.html dist/index.html",
    "build": "mkdir -p dist && cp src/index.html dist/index.html && node ./esbuild.js",
    "start": "npm run build:dev && esbuild src/index.js --serve --sourcemap --bundle --outdir=dist --servedir=dist",
    "start:relay": "node src/relay.js"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "rimraf": "^6.1.2"
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.2",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.1",
    "@headlessui/react": "^2.2.9",
    "@libp2p/autonat": "^3.0.9",
    "@libp2p/bootstrap": "^12.0.10",
    "@libp2p/circuit-relay-v2": "^4.1.2",
    "@libp2p/identify": "^4.0.9",
    "@libp2p/interface-pubsub": "^4.0.1",
    "@libp2p/logger": "^6.2.2",
    "@libp2p/peer-id-factory": "^4.2.4",
    "@libp2p/pubsub-peer-discovery": "^12.0.0",
    "@libp2p/tcp": "^11.0.9",
    "@libp2p/webrtc": "^6.0.10",
    "@libp2p/websockets": "^10.1.2",
    "@libp2p/webtransport": "^6.0.11",
    "@multiformats/multiaddr": "^13.0.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "date-fns": "^4.1.0",
    "libp2p": "^3.1.2",
    "npm-check-updates": "^19.3.1",
    "pretty-ms": "^9.3.0"
  }
}
